# Firebase Configuration
# Get these values from Firebase Console > Project Settings > General
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_firebase_app_id
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Google Maps API Keys
# Get these from Google Cloud Console > APIs & Services > Credentials
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
GOOGLE_MAPS_SERVER_API_KEY=your_google_maps_server_api_key_here

# Domino System Integration
# Contact your Domino system administrator for these values
NEXT_PUBLIC_DOMINO_API_URL=https://api.domino.example.com
NEXT_PUBLIC_DOMINO_API_KEY=your_domino_api_key_here

# Optional: OpenAI API Key (for AI features)
OPENAI_API_KEY=your_openai_api_key_here
